{
  "author": "PropLogix",
  "description": "Spelunker",
  "manifest_version": 2,
  "name": "Spelunker",
  "version": "0.1",
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js", "vendor.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.html"
  },
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  }
}
